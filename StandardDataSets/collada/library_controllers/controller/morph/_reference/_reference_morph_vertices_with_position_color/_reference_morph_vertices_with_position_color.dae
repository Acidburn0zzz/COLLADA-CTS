        </contributor>
        <created>2007-05-22T23:31:57Z</created>
        <keywords>Exemplary controller morph</keywords>
        <modified>2007-06-04T23:05:43Z</modified>
        <subject>Reference test where coordinates and colors are baked in to be equivalent to morph_vertices_with_position_color.</subject>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_cameras>
        <camera id="mainCamera" name="mainCamera">
            <optics>
                <technique_common>
                    <perspective>
                        <yfov>36.9998</yfov>
                        <aspect_ratio>1</aspect_ratio>
                        <znear>1</znear>
                        <zfar>1000</zfar>
                    </perspective>
                </technique_common>
            </optics>
        </camera>
    </library_cameras>
    <library_materials>
        <material id="lambert1" name="lambert1">
            <instance_effect url="#lambert1-fx"/>
        </material>
    </library_materials>
    <library_lights>
        <light id="directionalLightShape1-lib" name="directionalLightShape1">
            <technique_common>
                <directional>
                    <color>1 1 1</color>
                </directional>
            </technique_common>
        </light>
    </library_lights>
    <library_effects>
        <effect id="lambert1-fx">
            <profile_COMMON>
                <technique sid="common">
                    <lambert>
                        <diffuse>
                            <color>1 1 1 1</color>
                        </diffuse>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
    </library_effects>
    <library_geometries>
        <geometry id="cube3" name="cube3">
            <mesh>
                <source id="pCubeShape3-positions" name="position">
                    <float_array id="pCubeShape3-positions-array" count="9">-25 0 0 25 0 0 0 50 0</float_array>
                    <technique_common>
                        <accessor count="3" source="#pCubeShape3-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube-color3" name="color3">
                  <float_array id="cube-color3-array" count="9">0.5 0 0.5 0.5 0 0.5 0.5 0 0.5</float_array>
                  <technique_common>
                    <accessor count="3" source="#cube-color3-array" stride="3">
                      <param name="R" type="float"/>
                      <param name="G" type="float"/>
                      <param name="B" type="float"/>
                    </accessor>
                  </technique_common>
                </source>
                <vertices id="cube-vertices3">
                    <input semantic="POSITION" source="#pCubeShape3-positions"/>
                    <input semantic="COLOR" source="#cube-color3"/>
                </vertices>
                <polygons count="1" material="lambert2SG">
                    <input offset="0" semantic="VERTEX" source="#cube-vertices3"/>
                    <p>0 2 1</p>
                </polygons>
            </mesh>
        </geometry>
    </library_geometries>
    <library_visual_scenes>
        <visual_scene id="VisualSceneNode" name="untitled">
            <node id="cube_node3" name="cube_node3">
                <translate sid="translate">0 -50 0</translate>
                <scale>3 3 3</scale>
                <instance_geometry url="#cube3">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert2SG" target="#lambert1"/>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="testCamera" name="testCamera">
                <translate sid="translate">0 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 0</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_camera url="#mainCamera"/>
            </node>
            <node id="directionalLight1" name="directionalLight1">
                <translate sid="translate">40 0 40</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 15</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_light url="#directionalLightShape1-lib"/>
            </node>
        </visual_scene>
    </library_visual_scenes>
    <scene>
        <instance_visual_scene url="#VisualSceneNode"/>
    </scene>
</COLLADA>
