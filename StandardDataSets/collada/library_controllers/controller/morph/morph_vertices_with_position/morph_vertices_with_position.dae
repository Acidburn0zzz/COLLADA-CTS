                        </accessor>
                    </technique_common>
                </source>
                <source id="cube-color3" name="color3">
                  <float_array id="cube-color3-array" count="9">1 0 0 0 1 0 0 0 1</float_array>
                  <technique_common>
                    <accessor count="3" source="#cube-color3-array" stride="3">
