                    <input offset="0" semantic="VERTEX" source="#cube-vertices2"/>
                    <input offset="1" semantic="NORMAL" source="#pCubeShape2-normals"/>
                    <input offset="2" semantic="TEXCOORD" source="#cube-map2" set="0"/>
