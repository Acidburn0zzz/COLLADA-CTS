                    <input offset="1" semantic="NORMAL" source="#pCubeShape3-normals"/>
                    <input offset="2" semantic="TEXCOORD" source="#cube-map3" set="0"/>
                    <input offset="3" semantic="COLOR" source="#cube-color3"/>
                    <p>0 0 0 0 1 0 1 1 4 0 4 2</p>
                    <p>1 1 1 1 2 1 2 0 4 1 4 2</p>
                    <p>2 2 2 0 3 2 3 1 4 2 4 2</p>
                    <p>3 3 3 1 0 3 0 0 4 3 4 2</p>
                </polygons>
            </mesh>
        </geometry>
    </library_geometries>
    <library_controllers>
        <controller id="morph1">
            <morph source="#cube2">
                <source id="morph-targets">
                    <IDREF_array id="morph-targets-array" count="1">cube3</IDREF_array>
                    <technique_common>
                        <accessor count="1" source="#morph-targets-array">
                            <param name="MORPH_TARGET" type="IDREF"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="morph-weights">
                    <float_array id="morph-weights-array" count="1">0.75</float_array>
                    <technique_common>
                        <accessor count="1" source="#morph-weights-array">
                            <param name="MORPH_WEIGHT" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <targets>
                    <input semantic="MORPH_TARGET" source="#morph-targets"/>
                    <input semantic="MORPH_WEIGHT" source="#morph-weights"/>
                </targets>
            </morph>
        </controller>
    </library_controllers>
    <library_visual_scenes>
        <visual_scene id="VisualSceneNode" name="untitled">
            <node id="cube_node1" name="cube_node1">
                <translate sid="translate">-60 -60 0</translate>
                <scale sid="scale">2 2 2</scale>
                <instance_geometry url="#cube1">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert2SG" target="#lambert1">
                                <bind_vertex_input semantic="TEX0" input_semantic="TEXCOORD" input_set="0"/>
                            </instance_material>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="cube_node2" name="cube_node2">
                <translate sid="translate">60 60 0</translate>
                <scale sid="scale">2 2 2</scale>
                <instance_geometry url="#cube2">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert2SG" target="#lambert1">
                                <bind_vertex_input semantic="TEX0" input_semantic="TEXCOORD" input_set="0"/>
                            </instance_material>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="cube_node3" name="cube_node3">
                <translate sid="translate">-60 60 0</translate>
                <scale sid="scale">2 2 2</scale>
                <instance_geometry url="#cube3">
                    <bind_material>
