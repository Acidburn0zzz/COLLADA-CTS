						0 1 1 3 
						0 2 1 0 
						0 0 1 2 
						0 1 1 3 
						0 2 1 0
					</v>
				</vertex_weights>
			</skin>
		</controller>
		<controller id="controllers_1">
			<morph method="NORMALIZED" source="#controllers_0">
				<source id="controllers_0-MORPH_TARGET">
