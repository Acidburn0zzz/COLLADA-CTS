      # Distribution of this file or its content is strictly prohibited.
      </copyright>
        </contributor>
        <created>2007-05-22T23:31:57Z</created>
        <keywords>Exemplary controller morph</keywords>
        <modified>2007-06-04T23:05:43Z</modified>
        <subject>Reference test where coordinates, normals, textures, and colors are baked in to be equivalent to morph_vertices_with_position_normal_texture_color.</subject>
        <title>0133</title>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_cameras>
        <camera id="mainCamera" name="mainCamera">
            <optics>
                <technique_common>
                    <perspective>
                        <yfov>36.9998</yfov>
                        <aspect_ratio>1</aspect_ratio>
                        <znear>1</znear>
                        <zfar>1000</zfar>
                    </perspective>
                </technique_common>
            </optics>
        </camera>
    </library_cameras>
    <library_images>
        <image id="file1" name="file1">
            <init_from>
<ref>collada.png</ref>
</init_from>
        </image>
    <image id="file1-surface">
<init_from>
<ref>collada.png</ref>
</init_from>
</image>
</library_images>
    <library_materials>
        <material id="lambert1" name="lambert1">
            <instance_effect url="#lambert1-fx"/>
        </material>
    </library_materials>
    <library_lights>
        <light id="directionalLightShape1-lib" name="directionalLightShape1">
            <technique_common>
                <directional>
                    <color>1 1 1</color>
                </directional>
            </technique_common>
        </light>
    </library_lights>
    <library_effects>
        <effect id="lambert1-fx">
            <profile_COMMON>
                
                <newparam sid="file1-sampler">
                    <sampler2D>
                        <instance_image url="#file1-surface"/>
                    </sampler2D>
                </newparam>
                <technique sid="common">
                    <lambert>
                        <diffuse>
                            <texture texcoord="TEX0" texture="file1-sampler"/>
                        </diffuse>
                        <transparent opaque="RGB_ZERO">
                            <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>0.5</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
    </library_effects>
    <library_geometries>
        <geometry id="cube1" name="cube1">
            <mesh>
                <source id="cube1-positions" name="position">
                    <float_array count="12" id="cube1-positions-array">-20 0 0 -10 50 0 30 50 0 20 0 0</float_array>
                    <technique_common>
                        <accessor count="4" source="#cube1-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube1-normals" name="normal">
                    <float_array count="12" id="cube1-normals-array">0 0 0.5 0 0 0.5 0 0 0.5 0 0 0.5</float_array>
                    <technique_common>
                        <accessor count="4" source="#cube1-normals-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube1-map" name="map1">
                    <float_array count="8" id="cube1-map-array">0.38 0 0.38 1 0.88 1 0.88 0</float_array>
                    <technique_common>
                        <accessor count="4" source="#cube1-map-array" stride="2">
                            <param name="S" type="float"/>
                            <param name="T" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube1-color" name="color1">
                  <float_array count="12" id="cube1-color-array">0.25 0 0.75 0.75 0.25 0 0 0.75 0.25 0.25 0.75 1</float_array>
                  <technique_common>
                    <accessor count="4" source="#cube1-color-array" stride="3">
                      <param name="R" type="float"/>
                      <param name="G" type="float"/>
                      <param name="B" type="float"/>
                    </accessor>
                  </technique_common>
                </source>
                <vertices id="cube1-vertices">
                    <input semantic="POSITION" source="#cube1-positions"/>
                    <input semantic="NORMAL" source="#cube1-normals"/>
                    <input semantic="TEXCOORD" source="#cube1-map"/>
                    <input semantic="COLOR" source="#cube1-color"/>
                </vertices>
                <polygons count="1" material="lambert2SG">
                    <input offset="0" semantic="VERTEX" set="0" source="#cube1-vertices"/>
                    <p>0 3 2 1</p>
                </polygons>
            </mesh>
        </geometry>
    </library_geometries>
    <library_visual_scenes>
        <visual_scene id="VisualSceneNode" name="untitled">
            <node id="cube_node1" name="cube_node1">
                <translate sid="translate">0 -75 0</translate>
                <scale>3 3 3</scale>
                <instance_geometry url="#cube1">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert2SG" target="#lambert1"/>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="testCamera" name="testCamera">
                <translate sid="translate">0 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 0</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_camera url="#mainCamera"/>
            </node>
            <node id="directionalLight1" name="directionalLight1">
                <translate sid="translate">50 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 15</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_light url="#directionalLightShape1-lib"/>
            </node>
        </visual_scene>
    </library_visual_scenes>
    <scene>
        <instance_visual_scene url="#VisualSceneNode"/>
    </scene>
</COLLADA>
