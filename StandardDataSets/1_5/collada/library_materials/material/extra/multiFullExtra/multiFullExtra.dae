  </asset>
  <library_cameras>
    <camera id="mainCamera" name="mainCamera">
      <optics>
        <technique_common>
          <perspective>
            <yfov>36.9999</yfov>
            <aspect_ratio>1</aspect_ratio>
            <znear>1</znear>
            <zfar>1000</zfar>
          </perspective>
        </technique_common>
      </optics>
    </camera>
  </library_cameras>
  <library_lights>
    <light id="directionalLightShape1-lib" name="directionalLightShape1">
      <technique_common>
        <directional>
          <color>1 1 1</color>
        </directional>
      </technique_common>
    </light>
  </library_lights>
  <library_materials>
    <material id="lamb1" name="lamb1">
      <instance_effect url="#lamb1-fx"/>
      <extra>
        <technique profile="MAYA">
          <dynamic_attributes>
            <ilrIrradianceMult short_name="ilrIrradianceMult" type="color3">1 1 1</ilrIrradianceMult>
            <ilrGlossyRefractions short_name="ilrGlossyRefractions" type="bool">0</ilrGlossyRefractions>
            <ilrGlossyRefrMaximumRays short_name="ilrGlossyRefrMaximumRays" type="int">4</ilrGlossyRefrMaximumRays>
            <ilrGlossyRefrSharpness short_name="ilrGlossyRefrSharpness" type="float">4</ilrGlossyRefrSharpness>
            <ilrGlossyRefrAnisotropic short_name="ilrGlossyRefrAnisotropic" type="bool">0</ilrGlossyRefrAnisotropic>
            <ilrGlossyRefrAngle short_name="ilrGlossyRefrAngle" type="float">0</ilrGlossyRefrAngle>
            <ilrGlossyRefrSpreadX short_name="ilrGlossyRefrSpreadX" type="float">5</ilrGlossyRefrSpreadX>
            <ilrGlossyRefrSpreadY short_name="ilrGlossyRefrSpreadY" type="float">1</ilrGlossyRefrSpreadY>
          </dynamic_attributes>
        </technique>
      </extra>
    </material>
    <material id="lamb2" name="lamb2">
      <instance_effect url="#lamb2-fx"/>
      <extra>
        <technique profile="MAX">
          <dynamic_attributes>
            <ilrIrradianceMult short_name="ilrIrradianceMult" type="color3">1 1 1</ilrIrradianceMult>
            <ilrGlossyRefractions short_name="ilrGlossyRefractions" type="bool">0</ilrGlossyRefractions>
            <ilrGlossyRefrMaximumRays short_name="ilrGlossyRefrMaximumRays" type="int">4</ilrGlossyRefrMaximumRays>
            <ilrGlossyRefrSharpness short_name="ilrGlossyRefrSharpness" type="float">4</ilrGlossyRefrSharpness>
            <ilrGlossyRefrAnisotropic short_name="ilrGlossyRefrAnisotropic" type="bool">0</ilrGlossyRefrAnisotropic>
            <ilrGlossyRefrAngle short_name="ilrGlossyRefrAngle" type="float">0</ilrGlossyRefrAngle>
          </dynamic_attributes>
        </technique>
      </extra>
    </material>
    <material id="lamb3" name="lamb3">
      <instance_effect url="#lamb3-fx"/>
      <extra>
        <technique profile="FCOLLADA">
          <dynamic_attributes>
            <ilrIrradianceMult short_name="ilrIrradianceMult" type="color3">1 1 1</ilrIrradianceMult>
            <ilrGlossyRefractions short_name="ilrGlossyRefractions" type="bool">0</ilrGlossyRefractions>
            <ilrGlossyRefrMaximumRays short_name="ilrGlossyRefrMaximumRays" type="int">4</ilrGlossyRefrMaximumRays>
            <ilrGlossyRefrSharpness short_name="ilrGlossyRefrSharpness" type="float">4</ilrGlossyRefrSharpness>
          </dynamic_attributes>
        </technique>
      </extra>
    </material>
  </library_materials>
    <library_effects>
        <effect id="lamb1-fx">
            <profile_COMMON>
                <technique sid="common">
                    <lambert>
                        <emission>
                            <color>0 0 0 1</color>
                        </emission>
                        <ambient>
                            <color>0 0 0 1</color>
                        </ambient>
                        <diffuse>
                            <color>1 0 0 1</color>
                        </diffuse>
                        <transparent>
                            <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>1</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
        <effect id="lamb2-fx">
            <profile_COMMON>
                <technique sid="common">
                    <lambert>
                        <emission>
                            <color>0 0 0 1</color>
                        </emission>
                        <ambient>
                            <color>0 0 0 1</color>
                        </ambient>
                        <diffuse>
                            <color>0 1 0 1</color>
                        </diffuse>
                        <transparent>
                            <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>1</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
        <effect id="lamb3-fx">
            <profile_COMMON>
                <technique sid="common">
                    <lambert>
                        <emission>
                            <color>0 0 0 1</color>
                        </emission>
                        <ambient>
                            <color>0 0 0 1</color>
                        </ambient>
                        <diffuse>
                            <color>0 0 1 1</color>
                        </diffuse>
                        <transparent>
                            <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>1</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
    </library_effects>
    <library_geometries>
        <geometry id="pCubeShape1" name="pCubeShape1">
            <mesh>
                <source id="pCubeShape1-positions" name="position">
                    <float_array count="12" id="pCubeShape1-positions-array">-25 -25 0 25 -25 0 25 25 0 -25 25 0</float_array>
                    <technique_common>
                        <accessor count="4" source="#pCubeShape1-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="pCubeShape1-vertices">
                    <input semantic="POSITION" source="#pCubeShape1-positions"/>
                </vertices>
                <polygons count="1" material="initialShadingGroup1">
                    <input offset="0" semantic="VERTEX" source="#pCubeShape1-vertices"/>
                    <p>0 1 2 3</p>
                </polygons>
            </mesh>
        </geometry>
        <geometry id="triangleShape" name="triangleShape">
            <mesh>
                <source id="triangleShape-positions" name="position">
                    <float_array count="9" id="triangleShape-positions-array">0 25 0 -25 -25 0 25 -25 0</float_array>
                    <technique_common>
                        <accessor count="3" source="#triangleShape-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="triangleShape-vertices">
                    <input semantic="POSITION" source="#triangleShape-positions"/>
                </vertices>
                <triangles count="1" material="initialShadingGroup2">
                    <input offset="0" semantic="VERTEX" source="#triangleShape-vertices"/>
                    <p>0 1 2</p>
                </triangles>
            </mesh>
        </geometry>
        <geometry id="hexagonShape" name="hexagonShape">
            <mesh>
                <source id="hexagonShape-positions" name="position">
                    <float_array count="18" id="hexagonShape-positions-array">0 25 0 -25 15 0 -25 -15 0 0 -25 0 25 -15 0 25 15 0</float_array>
                    <technique_common>
                        <accessor count="6" source="#hexagonShape-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="hexagonShape-vertices">
                    <input semantic="POSITION" source="#hexagonShape-positions"/>
                </vertices>
                <polylist count="1" material="initialShadingGroup3">
                    <input offset="0" semantic="VERTEX" source="#hexagonShape-vertices"/>
                    <vcount>6</vcount>
		    <p>0 1 2 3 4 5</p>
                </polylist>
            </mesh>
        </geometry>
    </library_geometries>
  <library_visual_scenes>
    <visual_scene id="VisualSceneNode" name="checkerRGB">
      <node id="pCube1" name="pCube1">
	<translate>0 0 0</translate>
	<scale>1.5 1.5 1.5</scale>
	<instance_geometry url="#pCubeShape1">
	    <bind_material>
		<technique_common>
		    <instance_material symbol="initialShadingGroup1" target="#lamb1"/>
		</technique_common>
	    </bind_material>
	</instance_geometry>
      </node>
      <node id="triangle" name="triangle">
	<translate>-80 0 0</translate>
	<scale>1.5 1.5 1.5</scale>
	<instance_geometry url="#triangleShape">
	    <bind_material>
		<technique_common>
		    <instance_material symbol="initialShadingGroup2" target="#lamb2"/>
		</technique_common>
	    </bind_material>
	</instance_geometry>
      </node>
      <node id="hexagon" name="hexagon">
	<translate>80 0 0</translate>
	<scale>1.5 1.5 1.5</scale>
	<instance_geometry url="#hexagonShape">
	    <bind_material>
		<technique_common>
		    <instance_material symbol="initialShadingGroup3" target="#lamb3"/>
		</technique_common>
	    </bind_material>
	</instance_geometry>
      </node>
      <node id="testCamera" name="testCamera" type="NODE">
        <translate sid="translate">0 0 400</translate>
        <rotate sid="rotateZ">0 0 1 0</rotate>
        <rotate sid="rotateY">0 1 0 0</rotate>
        <rotate sid="rotateX">1 0 0 0</rotate>
        <instance_camera url="#mainCamera"/>
      </node>
      <node id="directionalLight1" name="directionalLight1" type="NODE">
        <translate sid="translate">50 0 400</translate>
        <rotate sid="rotateZ">0 0 1 0</rotate>
        <rotate sid="rotateY">0 1 0 15</rotate>
        <rotate sid="rotateX">1 0 0 0</rotate>
        <instance_light url="#directionalLightShape1-lib"/>
      </node>
    </visual_scene>
  </library_visual_scenes>
  <scene>
    <instance_visual_scene url="#VisualSceneNode"/>
  </scene>
</COLLADA>
