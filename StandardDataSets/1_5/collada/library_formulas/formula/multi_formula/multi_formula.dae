        <created>2011-01-11T18:07:57Z</created>
        <keywords>Exemplary library_formulas formula</keywords>
        <modified>2011-01-11T18:07:57Z</modified>
        <subject>Test preservation of a multiple formula element</subject>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_formulas>
        <formula id="formula1" sid="formula1_sid" name="formula1_name">
            <newparam sid="target1">
                <float>0</float>
            </newparam>
            <newparam sid="value">
                <float>0</float>
            </newparam>
            <newparam sid="pitch">
                <float>0</float>
            </newparam>
            <target>
                <param>target1</param>
            </target>
            <!-- target1 = (value/360) * pitch -->
            <technique_common>
                <math:math xmlns:math="http://www.w3.org/1998/Math/MathML">
                    <math:apply>
                        <math:times />
                        <math:apply>
                            <math:divide />
                            <math:csymbol encoding="COLLADA">value</math:csymbol>
                            <math:cn>360</math:cn>
                        </math:apply>
                        <math:csymbol encoding="COLLADA">pitch</math:csymbol>
                    </math:apply>
                </math:math>
            </technique_common>
            <technique profile="someProfile">
                <param>some data</param>
                <otherstuff type="someType">more data</otherstuff>
            </technique>
        </formula>
        <formula id="formula2" sid="formula2_sid" name="formula2_name">
            <newparam sid="target2">
                <float>0</float>
            </newparam>
            <newparam sid="value1">
                <float>0</float>
            </newparam>
            <newparam sid="value2">
                <float>0</float>
            </newparam>
            <target>
                <param>target</param>
            </target>
            <technique_common>
                <math:math xmlns:math="http://www.w3.org/1998/Math/MathML">
                    <math:apply>
                        <math:sin />
                        <math:apply>
                            <math:plus />
                            <math:apply>
                                <math:power />
                                <math:csymbol encoding="COLLADA">value1</math:csymbol>
                                <math:cn>2</math:cn>
                            </math:apply>
                            <math:csymbol encoding="COLLADA">value2</math:csymbol>
                        </math:apply>
                    </math:apply>
                </math:math>
            </technique_common>
        </formula>
    </library_formulas>
</COLLADA>
