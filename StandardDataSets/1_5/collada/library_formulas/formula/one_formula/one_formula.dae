        <subject>Test preservation of a single formula element</subject>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_formulas>
        <formula id="pythagorean" name="pythagorean">
            <newparam sid="hypotenuse">
                <float>0</float>
            </newparam>
            <newparam sid="side1">
                <int>0</int>
            </newparam>
            <newparam sid="side2">
                <int>0</int>
            </newparam>
            <target>
                <param>hypotenuse</param>
            </target>
            <technique_common>
                <math:math xmlns:math="http://www.w3.org/1998/Math/MathML">
                    <math:apply>
                    
		        <math:root/>
		        <math:degree><math:cn>2</math:cn></math:degree>
		        <math:apply>
                            <math:plus />
                            <math:apply>
                                <math:power />
                                <math:csymbol encoding="COLLADA">side1</math:csymbol>
                                <math:cn>2</math:cn>
                            </math:apply>
                            <math:apply>
                                <math:power />
                                <math:csymbol encoding="COLLADA">side2</math:csymbol>
                                <math:cn>2</math:cn>
                            </math:apply>
                        </math:apply>
                        
                    </math:apply>
                </math:math>
            </technique_common>
            <technique profile="someProfile">
                <param>some data</param>
                <otherstuff type="someType">more data</otherstuff>
            </technique>
        </formula>
    </library_formulas>
