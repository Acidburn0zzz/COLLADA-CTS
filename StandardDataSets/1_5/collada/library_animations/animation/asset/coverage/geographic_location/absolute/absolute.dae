        <created>2011-01-11T18:07:57Z</created>
        <keywords>Exemplary library_animations animation asset coverage</keywords>
        <modified>2011-01-11T18:07:57Z</modified>
        <subject>Test the preservation of the coverage element of an asset in an animation</subject>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_cameras>
        <camera id="mainCamera" name="mainCamera">
            <optics>
                <technique_common>
                    <perspective>
                        <yfov>36.9998</yfov>
                        <aspect_ratio>1</aspect_ratio>
                        <znear>1</znear>
                        <zfar>1000</zfar>
                    </perspective>
                </technique_common>
            </optics>
        </camera>
    </library_cameras>
  <library_lights>
    <light id="directionalLightShape1-lib" name="directionalLightShape1">
      <technique_common>
        <directional>
          <color>1 1 1</color>
        </directional>
      </technique_common>
    </light>
  </library_lights>
  <library_animations>
    <animation id="pCube1.translate" name="the animation name">
      <asset>
    	<coverage>
	    <geographic_location>
	        <longitude>-105.2830</longitude>
	        <latitude>40.0170</latitude>
	        <altitude mode="absolute">100</altitude>
	    </geographic_location>
    	</coverage>
        <created>2011-01-11T18:07:57Z</created>
        <modified>2011-02-01T23:09:47Z</modified>
      </asset>
      <source id="pCube1.translate_pCube1_translate.Y-input">
        <float_array id="pCube1.translate_pCube1_translate.Y-input-array" count="2">0 1</float_array>
        <technique_common>
          <accessor source="#pCube1.translate_pCube1_translate.Y-input-array" count="2" stride="1">
            <param name="TIME" type="float"/>
          </accessor>
        </technique_common>
      </source>
      <source id="pCube1.translate_pCube1_translate.Y-output">
        <float_array id="pCube1.translate_pCube1_translate.Y-output-array" count="2">-5 5</float_array>
        <technique_common>
          <accessor source="#pCube1.translate_pCube1_translate.Y-output-array" count="2" stride="1">
            <param name="Y" type="float"/>
          </accessor>
        </technique_common>
      </source>
      <source id="pCube1.translate_pCube1_translate.Y-interpolations">
        <Name_array id="pCube1.translate_pCube1_translate.Y-interpolations-array" count="2">BEZIER BEZIER</Name_array>
        <technique_common>
          <accessor source="#pCube1.translate_pCube1_translate.Y-interpolations-array" count="2" stride="1">
            <param name="INTERPOLATION" type="Name"/>
          </accessor>
        </technique_common>
      </source>
      <source id="pCube1.translate_pCube1_translate.Y-intangents">
        <float_array id="pCube1.translate_pCube1_translate.Y-intangents-array" count="4">-0.333333 -16.6667 0.666667 3.33333</float_array>
        <technique_common>
          <accessor source="#pCube1.translate_pCube1_translate.Y-intangents-array" count="2" stride="2">
            <param name="X" type="float"/>
            <param name="Y" type="float"/>
          </accessor>
        </technique_common>
      </source>
      <source id="pCube1.translate_pCube1_translate.Y-outtangents">
        <float_array id="pCube1.translate_pCube1_translate.Y-outtangents-array" count="4">0.333333 -3.33333 1.33333 16.6667</float_array>
        <technique_common>
          <accessor source="#pCube1.translate_pCube1_translate.Y-outtangents-array" count="2" stride="2">
            <param name="X" type="float"/>
            <param name="Y" type="float"/>
          </accessor>
        </technique_common>
      </source>
      <sampler id="pCube1.translate_pCube1_translate.Y-sampler">
        <input semantic="INPUT" source="#pCube1.translate_pCube1_translate.Y-input"/>
        <input semantic="OUTPUT" source="#pCube1.translate_pCube1_translate.Y-output"/>
        <input semantic="INTERPOLATION" source="#pCube1.translate_pCube1_translate.Y-interpolations"/>
        <input semantic="IN_TANGENT" source="#pCube1.translate_pCube1_translate.Y-intangents"/>
        <input semantic="OUT_TANGENT" source="#pCube1.translate_pCube1_translate.Y-outtangents"/>
      </sampler>
      <channel source="#pCube1.translate_pCube1_translate.Y-sampler" target="pCube1/translate.Y"/>
    </animation>
  </library_animations>
  <library_materials>
    <material id="lambert2" name="lambert2">
      <instance_effect url="#lambert2-fx"/>
    </material>
  </library_materials>
  <library_effects>
    <effect id="lambert2-fx">
      <profile_COMMON>
        <technique sid="common">
          <constant>
            <emission>
              <color>0.4784 0.103 0.054059 0.8</color>
            </emission>
          </constant>
        </technique>
      </profile_COMMON>
    </effect>
  </library_effects>
  <library_geometries>
    <geometry id="pCubeShape1" name="pCubeShape1">
      <mesh>
        <source id="pCubeShape1-positions" name="position">
          <float_array id="pCubeShape1-positions-array" count="24">-1.5 -1.5 1.5 1.5 -1.5 1.5 -1.5 1.5 1.5 1.5 1.5 1.5 -1.5 1.5 -1.5 1.5 1.5 -1.5 -1.5 -1.5 -1.5 1.5 -1.5 -1.5</float_array>
          <technique_common>
            <accessor source="#pCubeShape1-positions-array" count="8" stride="3">
              <param name="X" type="float"/>
              <param name="Y" type="float"/>
              <param name="Z" type="float"/>
            </accessor>
          </technique_common>
        </source>
        <vertices id="pCubeShape1-vertices">
          <input semantic="POSITION" source="#pCubeShape1-positions"/>
        </vertices>
        <polylist material="lambert2SG" count="6">
          <input semantic="VERTEX" source="#pCubeShape1-vertices" offset="0"/>
          <vcount>4 4 4 4 4 4</vcount>
          <p>0 1 3 2 2 3 5 4 4 5 7 6 6 7 1 0 1 7 5 3 6 0 2 4</p>
        </polylist>
      </mesh>
    </geometry>
  </library_geometries>
  <library_visual_scenes>
    <visual_scene id="VisualSceneNode" name="untitled">
      <node id="pCube1" name="pCube1" type="NODE">
        <translate sid="translate">0 0 370</translate>
        <scale>5 1 1</scale>
        <instance_geometry url="#pCubeShape1">
          <bind_material>
            <technique_common>
              <instance_material symbol="lambert2SG" target="#lambert2"/>
            </technique_common>
          </bind_material>
        </instance_geometry>
      </node>
      <node id="testCamera" name="testCamera" type="NODE">
        <translate sid="translate">0 0 400</translate>
        <rotate sid="rotateZ">0 0 1 0</rotate>
        <rotate sid="rotateY">0 1 0 0</rotate>
        <rotate sid="rotateX">1 0 0 0</rotate>
        <instance_camera url="#mainCamera"/>
      </node>
      <node id="directionalLight1" name="directionalLight1" type="NODE">
        <translate sid="translate">50 0 400</translate>
        <rotate sid="rotateZ">0 0 1 0</rotate>
        <rotate sid="rotateY">0 1 0 15</rotate>
        <rotate sid="rotateX">1 0 0 0</rotate>
        <instance_light url="#directionalLightShape1-lib"/>
      </node>
    </visual_scene>
  </library_visual_scenes>
  <scene>
    <instance_visual_scene url="#VisualSceneNode"/>
  </scene>
</COLLADA>
