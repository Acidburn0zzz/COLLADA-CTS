		<keywords>Kinematics_Baseline library_kinematics_model kinematics_model technique_common joint revolute</keywords>
		<modified>2010-03-04T15:17:37Z</modified>
		<subject>Test preservation of the limits element of a prismatic joint in kinematics_model</subject>
		<unit meter="0.001" name="millimeter"/>
		<up_axis>Y_UP</up_axis>
	</asset>
	<library_kinematics_models>
		<kinematics_model id="KIN_GREIFER">
			<technique_common>
				<joint id="prismatic_joint">
					<prismatic sid="axis1">
						<axis>1 0 0</axis>
						<limits>
							<min>-500</min>
							<max>0</max>
						</limits>
					</prismatic>
					<prismatic sid="axis2">
						<axis>0 1 0</axis>
						<limits>
							<min>-400</min>
						</limits>
					</prismatic>
					<prismatic sid="axis3">
						<axis>0 0 1</axis>
						<limits>
							<max>200</max>
						</limits>
					</prismatic>
				</joint>
				<instance_joint url="#prismatic_joint" sid="j1"/>
				<link>
					<attachment_full joint="KIN_GREIFER/j1">
						<translate>525 0 0</translate>
						<link/>
					</attachment_full>
				</link>
			</technique_common>
		</kinematics_model>
	</library_kinematics_models>
</COLLADA>
