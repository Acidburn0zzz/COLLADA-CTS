    <library_cameras>
        <camera id="mainCamera" name="mainCamera">
            <optics>
                <technique_common>
                    <perspective>
                        <yfov>36.9999</yfov>
                        <aspect_ratio>1</aspect_ratio>
                        <znear>1</znear>
                        <zfar>1000</zfar>
                    </perspective>
                </technique_common>
            </optics>
        </camera>
    </library_cameras>
    <library_lights>
        <light id="directionalLightShape1-lib" name="directionalLightShape1">
            <technique_common>
                <directional>
                    <color>1 1 1</color>
                </directional>
            </technique_common>
        </light>
    </library_lights>
    <library_materials>
        <material id="constant1" name="constant1">
            <instance_effect url="#constant1-fx"/>
        </material>
    </library_materials>
    <library_effects>
        <effect id="constant1-fx">
            <profile_COMMON>
                <technique sid="common">
                    <constant>
                        <emission>
                            <color>0.5 0.5 0.5 1</color>
                        </emission>
                    </constant>
                </technique>
            </profile_COMMON>
        </effect>
    </library_effects>
    <library_geometries>
        <geometry id="pCubeShape1" name="pCubeShape1">
            <mesh>
                <source id="pCubeShape1-positions" name="position">
                    <float_array count="18" id="pCubeShape1-positions-array">-5 -10 0 5 -10 0 10 0 0 5 10 0 -5 10 0 -10 0 0</float_array>
                    <technique_common>
                        <accessor count="6" source="#pCubeShape1-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="pCubeShape1-vertices">
                    <input semantic="POSITION" source="#pCubeShape1-positions"/>
                </vertices>
                <polylist count="1" material="initialShadingGroup">
                    <input offset="0" semantic="VERTEX" source="#pCubeShape1-vertices"/>
                    <vcount>6</vcount>
                    <p>0 1 2 3 4 5</p>
                </polylist>
            </mesh>
        </geometry>
    </library_geometries>
    <library_visual_scenes>
        <visual_scene id="VisualSceneNode" name="one_polygon">
            <node id="pCube1" name="pCube1">
                <scale>7 7 7</scale>
                <instance_geometry url="#pCubeShape1">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="initialShadingGroup" target="#constant1"/>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="testCamera" name="testCamera">
                <translate sid="translate">0 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 0</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_camera url="#mainCamera"/>
            </node>
            <node id="directionalLight1" name="directionalLight1">
                <translate sid="translate">50 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 15</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_light url="#directionalLightShape1-lib"/>
            </node>
        </visual_scene>
    </library_visual_scenes>
    <scene>
        <instance_visual_scene url="#VisualSceneNode"/>
    </scene>
