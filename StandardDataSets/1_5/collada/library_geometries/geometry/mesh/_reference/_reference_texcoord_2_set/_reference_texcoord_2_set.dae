	<subject>Reference test case for multiple texture sets</subject>
        <title>0396</title>
        <unit meter="0.01" name="centimeter"/>
        <up_axis>Y_UP</up_axis>
    </asset>
    <library_cameras>
        <camera id="mainCamera" name="mainCamera">
            <optics>
                <technique_common>
                    <perspective>
                        <yfov>36.9998</yfov>
                        <aspect_ratio>1</aspect_ratio>
                        <znear>1</znear>
                        <zfar>1000</zfar>
                    </perspective>
                </technique_common>
            </optics>
        </camera>
    </library_cameras>
    <library_lights>
        <light id="directionalLightShape1-lib" name="directionalLightShape1">
            <technique_common>
                <directional>
                    <color>1 1 1</color>
                </directional>
            </technique_common>
        </light>
    </library_lights>
    <library_images>
        <image id="file1" name="file1">
            <init_from>
<ref>collada.png</ref>
</init_from>
        </image>
        <image id="file2" name="file2">
            <init_from>
<ref>checkerboard.png</ref>
</init_from>
        </image>
    </library_images>
    <library_materials>
        <material id="lambert1" name="lambert2">
            <instance_effect url="#lambert1-fx"/>
        </material>
        <material id="lambert2" name="lambert1">
            <instance_effect url="#lambert2-fx"/>
        </material>
    </library_materials>
    <library_effects>
        <effect id="lambert1-fx">
            <profile_COMMON>
                
                <newparam sid="file1-sampler">
                    <sampler2D>
                        <instance_image url="#file1"/>
                    </sampler2D>
                </newparam>
                <technique sid="common">
                    <lambert>
                        <emission>
                            <color>0 0 0 1</color>
                        </emission>
                        <ambient>
                            <color>0 0 0 1</color>
                        </ambient>
                        <diffuse>
                            <texture texcoord="TEX0" texture="file1-sampler"/>
                        </diffuse>
                        <transparent>
                            <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>1</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
        <effect id="lambert2-fx">
            <profile_COMMON>
                
                <newparam sid="file2-sampler">
                    <sampler2D>
                        <instance_image url="#file2"/>
                    </sampler2D>
                </newparam>
                <technique sid="common">
                    <lambert>
                        <emission>
                            <color>0 0 0 1</color>
                        </emission>
                        <ambient>
                            <color>0 0 0 1</color>
                        </ambient>
                           <diffuse>
                               <texture texcoord="TEX1" texture="file2-sampler"/>
                           </diffuse>
                           <transparent>
                               <color>0 0 0 1</color>
                        </transparent>
                        <transparency>
                            <float>1</float>
                        </transparency>
                    </lambert>
                </technique>
            </profile_COMMON>
        </effect>
    </library_effects>
    <library_geometries>
        <geometry id="cube1" name="cube1">
            <mesh>
                <source id="pCubeShape1-positions" name="position">
                    <float_array count="12" id="pCubeShape1-positions-array">-25 -25 25 25 -25 25 -25 25 25 25 25 25</float_array>
                    <technique_common>
                        <accessor count="4" source="#pCubeShape1-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube1-map" name="map1">
                    <float_array count="8" id="cube1-map-array">0.25 0.25 0.75 0.25 0.75 0.75 0.25 0.75</float_array>
                    <technique_common>
                        <accessor count="4" source="#cube1-map-array" stride="2">
                            <param name="S" type="float"/>
                            <param name="T" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="cube1-vertices">
                    <input semantic="POSITION" source="#pCubeShape1-positions"/>
                </vertices>
                <polylist count="1" material="lambert1SG">
                    <input offset="0" semantic="VERTEX" source="#cube1-vertices"/>
                    <input offset="1" semantic="TEXCOORD" set="0" source="#cube1-map"/>
                    <vcount>4</vcount>
                    <p>0 0 1 1 3 2 2 3</p>
                </polylist>
            </mesh>
        </geometry>
        <geometry id="cube2" name="cube2">
            <mesh>
                <source id="pCubeShape2-positions" name="position">
                    <float_array count="12" id="pCubeShape2-positions-array">-25 -25 25 25 -25 25 -25 25 25 25 25 25</float_array>
                    <technique_common>
                        <accessor count="4" source="#pCubeShape2-positions-array" stride="3">
                            <param name="X" type="float"/>
                            <param name="Y" type="float"/>
                            <param name="Z" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <source id="cube2-map" name="map2">
                    <float_array count="8" id="cube2-map-array">0 0 1 0 1 1 0 1</float_array>
                    <technique_common>
                        <accessor count="4" source="#cube2-map-array" stride="2">
                            <param name="S" type="float"/>
                            <param name="T" type="float"/>
                        </accessor>
                    </technique_common>
                </source>
                <vertices id="cube2-vertices">
                    <input semantic="POSITION" source="#pCubeShape2-positions"/>
                </vertices>
                <polylist count="1" material="lambert2SG">
                    <input offset="0" semantic="VERTEX" source="#cube2-vertices"/>
                    <input offset="1" semantic="TEXCOORD" set="1" source="#cube2-map"/>
                    <vcount>4</vcount>
                    <p>0 0 1 1 3 2 2 3</p>
                </polylist>
            </mesh>
        </geometry>
    </library_geometries>
    <library_visual_scenes>
        <visual_scene id="VisualSceneNode" name="untitled">
            <node id="cube_node" name="cube_node">
                <translate>-55 0 0</translate>
                <scale sid="scale">2 2 2</scale>
                <instance_geometry url="#cube1">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert1SG" target="#lambert1">
                                <bind_vertex_input input_semantic="TEXCOORD" input_set="0" semantic="TEX0"/>
                            </instance_material>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="cube_node2" name="cube_node2">
                <translate>55 0 0</translate>
                <scale sid="scale">2 2 2</scale>
                <instance_geometry url="#cube2">
                    <bind_material>
                        <technique_common>
                            <instance_material symbol="lambert2SG" target="#lambert2">
                                <bind_vertex_input input_semantic="TEXCOORD" input_set="1" semantic="TEX1"/>
                            </instance_material>
                        </technique_common>
                    </bind_material>
                </instance_geometry>
            </node>
            <node id="testCamera" name="testCamera">
                <translate sid="translate">0 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 0</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_camera url="#mainCamera"/>
            </node>
            <node id="directionalLight1" name="directionalLight1">
                <translate sid="translate">50 0 400</translate>
                <rotate sid="rotateZ">0 0 1 0</rotate>
                <rotate sid="rotateY">0 1 0 15</rotate>
                <rotate sid="rotateX">1 0 0 0</rotate>
                <instance_light url="#directionalLightShape1-lib"/>
            </node>
        </visual_scene>
    </library_visual_scenes>
    <scene>
        <instance_visual_scene url="#VisualSceneNode"/>
    </scene>
</COLLADA>
