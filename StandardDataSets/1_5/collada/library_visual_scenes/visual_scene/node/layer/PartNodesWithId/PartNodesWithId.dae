  <library_cameras>
    <camera id="mainCamera">
      <optics>
        <technique_common>
          <perspective>
            <xfov>36.9999</xfov>
            <aspect_ratio>1</aspect_ratio>
            <znear>0.393701</znear>
            <zfar>393.701</zfar>
          </perspective>
        </technique_common>
      </optics>
    </camera>
  </library_cameras>
  <library_lights>
